      <Trigger Enabled="true" Name="%action_3%" Id="%id_action_2%" RegularExpression="^.{14} StartsCasting 14:[^:]*:(?&lt;player&gt;[^:]+?):[^:]*:%action_2%" PrevActions="Interrupt">
