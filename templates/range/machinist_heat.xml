      <Trigger Enabled="true" Name="%action_1%-%action_2%" Id="%id_action_1%" RegularExpression="^.{14} Gauge 1F:[^:]*:[^:]{0,2}(?&lt;heatms&gt;(1F40|0000))?1F:" PrevActions="Interrupt">
        <Actions>
          <Action OrderNumber="1" LogLevel="-1" NamedCallbackName="command" NamedCallbackParam="/%is_cross%hotbar set &quot;%action_2%&quot; %position% " TextAuraFontSize="9" TextAuraFontName="宋体" ActionType="NamedCallback" RefireInterrupt="True">
            <Condition Enabled="true" Grouping="Or">
              <ConditionSingle Enabled="true" ExpressionL="${heatms}" ExpressionTypeL="String" ExpressionR="1F40" ExpressionTypeR="String" ConditionType="StringEqualNocase" />
            </Condition>
            <Conditions />
          </Action>
          <Action OrderNumber="2" LogLevel="-1" NamedCallbackName="command" NamedCallbackParam="/%is_cross%hotbar set &quot;%action_1%&quot; %position% " TextAuraFontSize="9" TextAuraFontName="宋体" ActionType="NamedCallback" RefireInterrupt="True">
            <Condition Enabled="true" Grouping="Or">
              <ConditionSingle Enabled="true" ExpressionL="${heatms}" ExpressionTypeL="String" ExpressionR="1F40" ExpressionTypeR="String" ConditionType="StringNotEqualNocase" />
            </Condition>
            <Conditions />
          </Action>
        </Actions>
        <Condition Enabled="false" Grouping="Or" />
        <Conditions />
      </Trigger>
    </Triggers>
  </ExportedFolder>
</TriggernometryExport>